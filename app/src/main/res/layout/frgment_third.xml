<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"

    android:layout_height="match_parent"

    tools:layout_editor_absoluteY="81dp">


    <TextView

        android:id="@+id/wodeshoucang"

        android:layout_width="368dp"

        android:layout_height="45dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="120dp"

        android:layout_marginEnd="8dp"

        android:text="我的收藏"

        android:textSize="24sp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView

        android:id="@+id/wodejifen"

        android:layout_width="match_parent"

        android:layout_height="50dp"

        android:layout_marginStart="8dp"

        android:layout_marginLeft="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginRight="8dp"

        android:text="我的积分"

        android:textSize="24sp"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.562"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/wodeshoucang" />



    <TextView

        android:id="@+id/wodefabu"

        android:layout_width="0dp"

        android:layout_height="50dp"

        android:layout_marginStart="8dp"

        android:layout_marginLeft="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginRight="8dp"

        android:text="我的发布"

        android:textSize="24sp"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/wodejifen" />



    <TextView

        android:id="@+id/wodehsngchuan"

        android:layout_width="0dp"

        android:layout_height="50dp"

        android:layout_marginStart="8dp"

        android:layout_marginLeft="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginRight="8dp"

        android:text="我的上传"

        android:textSize="24sp"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/wodefabu" />


    <TextView

        android:id="@+id/wodexiazai"

        android:layout_width="0dp"

        android:layout_height="50dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:text="我的下载"

        android:textSize="24sp"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.0"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/wodehsngchuan" />



    <TextView

        android:id="@+id/nichen"

        android:layout_width="103dp"

        android:layout_height="26dp"

        android:layout_marginStart="8dp"

        android:layout_marginLeft="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginRight="8dp"

        android:text="昵称"

        android:visibility="visible"

        app:layout_constraintEnd_toStartOf="@+id/shezhi"

        app:layout_constraintStart_toEndOf="@+id/touxiang"

        app:layout_constraintTop_toTopOf="parent" />



    <ImageView

        android:id="@+id/touxiang"

        android:layout_width="114dp"

        android:layout_height="98dp"

        android:layout_marginStart="8dp"

        android:layout_marginLeft="8dp"

        app:layout_constraintStart_toStartOf="parent"

        app:srcCompat="@drawable/image" />



    <ImageView

        android:id="@+id/shezhi"

        android:layout_width="106dp"

        android:layout_height="68dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginRight="8dp"

        android:layout_marginBottom="8dp"

        app:layout_constraintBottom_toBottomOf="@+id/touxiang"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toTopOf="parent"

        app:srcCompat="@drawable/setting" />


    <TextView

        android:id="@+id/idhao"

        android:layout_width="105dp"

        android:layout_height="26dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginBottom="8dp"

        android:text="TextView"

        app:layout_constraintBottom_toTopOf="@+id/wodeshoucang"

        app:layout_constraintEnd_toStartOf="@+id/shezhi"

        app:layout_constraintStart_toEndOf="@+id/touxiang"

        app:layout_constraintTop_toBottomOf="@+id/nichen" />


</android.support.constraint.ConstraintLayout>

